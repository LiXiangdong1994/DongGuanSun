<script>
    $(function(){
        $('#MainTables').on('click', function () {
            $(window.parent.document).find('#contentIframe').attr('src', 'tables.aspx')
            $(this).addClass('layui-this').siblings().removeClass('layui-this');
        })
        var UserID="<%=Session["UserID"]%>";
        if(UserID!=""){
            var UserID = UserID;
            $("#UserName").html(UserID);
        }else{
            $("#UserName").html("用户未登陆");
        }
    })
   //显示样式
    $('.Title-Change').on('click', function () {
        $(".title_change").html($(this).find(".menu-text").html());
        $(".small-Title").html($(this).find(".menu-text").html());
       
    })
    $('.node').on('click', function () {
        $(".small-small-Title").html($(this).find("span").html());

    })
    
</script>